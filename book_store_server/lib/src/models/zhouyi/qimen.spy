class: Qimen
table: qimen_history

fields:
  userId: int
  ### 排盘时间
  panTime: DateTime
  ### 排盘方式（如奇门遁甲、四柱八字等）
  method: String
  ### 遁法（阳遁、阴遁）
  dunType: String
  ### # 局数（1~9）
  juShu: int
  ### 占事（用户所问之事）
  question: String
  ### 断言（断语或推演结果）
  analysis: String
  ### 记录创建时间
  createTime: DateTime, default = now
  ### 记录最后更新时间
  updateTime: DateTime, default = now
